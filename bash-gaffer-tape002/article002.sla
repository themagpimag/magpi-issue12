            <ITEXT FONTSIZE="9" CH=" #!/bin/bash"/>
            <para PARENT="Code"/>
            <ITEXT FONTSIZE="9" CH=" # A script to gzip all of the files in the present working"/>
            <para PARENT="Code"/>

"1" Pagenumber="0" isInline="0" fillRule="1" doOverprint="0" gXpos="0" gYpos="0" gWidth="0" gHeight="0" LAYER="6" BOOKMARK="0" NEXTITEM="-1" BACKITEM="-1">
            <ITEXT FONTSIZE="10" CH="1. #!/usr/bin/env python"/>
            <para PARENT="code"/>
            <ITEXT FONTSIZE="10" CH="2. # Print a file to the printer"/>
            <para PARENT="code"/>
            <ITEXT FONTSIZE="10" CH="3. "/>
            <para PARENT="code"/>
            <ITEXT CH="4. import cups"/>
            <para PARENT="code"/>
            <ITEXT CH="5. from xhtml2pdf import pisa"/>
            <para PARENT="code"/>
            <ITEXT CH="6."/>
            <tab/>
            <para PARENT="code"/>
            <ITEXT CH="7. def main():"/>
            <para PARENT="code"/>
            <ITEXT CH="8.   # Filename for temp file"/>
            <para PARENT="code"/>
            <ITEXT CH="9.   filename = &quot;/home/pi/print.pdf&quot;"/>
            <para PARENT="code"/>
            <ITEXT CH="10."/>
            <tab/>
            <para PARENT="code"/>
            <ITEXT CH="11.  # generate content"/>
            <para PARENT="code"/>
            <ITEXT CH="12.  xhtml = &quot;&lt;h1&gt;Test print&lt;/h1&gt;\n&quot;"/>
            <para PARENT="code"/>
            <ITEXT CH="13.  xhtml += &quot;&lt;h2&gt;This is printed from "/>
            <para PARENT="code"/>
            <ITEXT CH="     within a Python application&lt;/h2&gt;\n&quot;"/>
            <para PARENT="code"/>
            <ITEXT CH="14.  xhtml += &quot;&lt;p style=\&quot;color:red;\&quot;&gt;"/>
            <para PARENT="code"/>
            <ITEXT CH="     Coloured red using css&lt;/p&gt;\n&quot;"/>
            <para PARENT="code"/>
            <ITEXT CH="15.          "/>
            <para PARENT="code"/>
            <ITEXT CH="16.  pdf = pisa.CreatePDF(xhtml, "/>
            <para PARENT="code"/>
            <ITEXT CH="     file(filename, &quot;w&quot;))"/>
            <para PARENT="code"/>
            <ITEXT CH="17.  if not pdf.err:"/>
            <para PARENT="code"/>
            <ITEXT CH="18.    # Close PDF file - otherwise we can't "/>
            <para PARENT="code"/>
            <ITEXT CH="       read it"/>
            <para PARENT="code"/>
            <ITEXT CH="19.    pdf.dest.close()"/>
            <para PARENT="code"/>
            <ITEXT CH="20.      "/>
            <para PARENT="code"/>
            <ITEXT CH="21.    # print the file using cups"/>
            <para PARENT="code"/>
            <ITEXT CH="22.    conn = cups.Connection()"/>
            <para PARENT="code"/>
            <ITEXT CH="23.    # Get a list of all printers"/>
            <para PARENT="code"/>
            <ITEXT CH="24.    printers = conn.getPrinters()"/>
            <para PARENT="code"/>
            <ITEXT CH="25.    for printer in printers: "/>
            <para PARENT="code"/>
            <ITEXT CH="26.      # Print name of printers to stdout "/>
            <para PARENT="code"/>
            <ITEXT CH="         (screen)"/>
            <para PARENT="code"/>
            <ITEXT CH="27.      print printer, "/>
            <para PARENT="code"/>
            <ITEXT CH="         printers[printer][&quot;device-uri&quot;]"/>
            <para PARENT="code"/>
            <ITEXT CH="28.    # get first printer from printer list"/>
            <para PARENT="code"/>
            <ITEXT CH="29.    printer_name = printers.keys()[0]"/>
            <para PARENT="code"/>
            <ITEXT CH="30.    conn.printFile(printer_name, filename,"/>
            <para PARENT="code"/>
            <ITEXT CH="       &quot;Python_Status_print&quot;, {})"/>
            <para PARENT="code"/>
            <ITEXT CH="31.  else:"/>
            <para PARENT="code"/>
            <ITEXT CH="32.    print &quot;Unable to create pdf file&quot;"/>
            <para PARENT="code"/>
            <ITEXT CH="33."/>
            <para PARENT="code"/>
            <ITEXT CH="34. if __name__==&quot;__main__&quot;:"/>
            <para PARENT="code"/>
            <ITEXT CH="35.   main()"/>
            <trail PARENT="code"/>
